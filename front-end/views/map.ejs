                </a>
                <a href="#" class="sidebar-toggler flex-shrink-0">
                    <i class="fa fa-bars"></i>
                </a>
            </nav>
            <!-- Navbar End -->


            <!-- Map Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="row g-2">
                    <h4 class="mb-0">Geographical Distribution of Users</h4>
                    <div class="col-12" id="svgMap">
                    </div>
                </div>
            </div>
            <!-- Map End -->



            <!-- Footer Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="bg-secondary rounded-top p-4">
                    <div class="row">
                        <div class="col-12 col-sm-6 text-center text-sm-end">
                             Designed By <a href="https://github.sfu.ca/tjd10/T.U.B.A">Team T.U.B.A</a>
                            <br>For CMPT 732 Final Project</a>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Footer End -->
        </div>
